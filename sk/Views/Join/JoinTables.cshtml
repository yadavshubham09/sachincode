

@{
    ViewBag.Title = "JoinTables";
}

<h2>JoinTables</h2>

<table>
    <thead>
        <tr>
            <th>Employee ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Department</th>
            <th>Salary</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.EmployeeID</td>
                @*<td>@item.Employee.FirstName</td>
                    <td>@item.Employee.LastName</td>
                    <td>@item.Department.DepartmentName</td>
                    <td>@item.Employee.Salary</td>*@
            </tr>
        }
    </tbody>
</table>
